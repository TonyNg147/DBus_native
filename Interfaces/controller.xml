<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="controller.service">
    <property name="status" type="b" access="read"/>
    <signal name="statusChanged">
    </signal>
    <method name="changeStatus">
      <arg name="status" type="b" direction="in"/>
    </method>
        <method name="sendArg">                                                                                                                                                   
        <arg type="is" direction="out">                                                                                 
        <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="DataTransfer">                                                                                 
    </method>
  </interface>
</node>
